<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="true" />

	<package name="basic-action-mappings" extends="struts-default">
		<action name="main_page">
			<result>WEB-INF/main_page.jsp</result>
		</action>
		<action name="asset_list">
			<result>WEB-INF/asset_list.jsp</result>
		</action>
		<action name="asset_details">
			<result>WEB-INF/asset_details.jsp</result>
		</action>
	</package>
</struts>